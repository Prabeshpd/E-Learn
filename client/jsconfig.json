{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@Components/*": ["./src/components/*"],
      "@Services/*": ["./src/services/*"],
      "@Utils/*": ["./src/utils/*"],
      "@Actions/*": ["./src/actions/*"],
      "@Commons/*": ["./src/common/*"],
      "@Assets/*": ["./src/assets/*"],
      "@Styles/*": ["./src/styles/*"]
    }
  },
  //Add any build/compiled folders here to stop vscode searching those
  "exclude": ["node_modules", "build"]
}
